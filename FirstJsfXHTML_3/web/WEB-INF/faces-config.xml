<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>de</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>fa</supported-locale>
        </locale-config>
    </application>
    
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>marke1</from-outcome>
            <to-view-id>/zweite.jsp</to-view-id>
            
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>marke2</from-outcome>vvvvvvvfv
            <to-view-id>/dritte.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <to-view-id>/default.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
